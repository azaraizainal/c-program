#include<stdio.h>
int main()
{
   int a, b;
   printf("Please give a number as 1st variable:\n");
   scanf("%d",&b);
   printf("Please give a number as 2nd variable:\n");
   scanf("%d",&a);

   printf("The value of the 1st variable: %d\n",a);
   printf("The value of the 2nd variable: %d\n",b);

   return 0;
}
